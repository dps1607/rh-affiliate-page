{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "functions": {
    "api/hello.js": {
      "maxDuration": 30
    },
    "api/test.js": {
      "maxDuration": 30
    },
    "api/index.js": {
      "maxDuration": 30
    },
    "api/simple.js": {
      "maxDuration": 30
    }
  }
}
